    <div id="addplayer">
        <p>Add a new player:</p>
        <form>
            <label for="name">Name:</label>
            <input type="text" id="name" value="name">
            <label for="birthdate">Birthdate:</label>
            <input type="date" id="birthdate" value="birthdate">
            <label for="debut">Debut:</label>
            <input type="date" id="debut" value="debut">
            <label for="number">Jersey Number</label>
            <input type="number" id="number" value="number">
            <label for="team">Team</label>
            <input type="number" id="team" value="team">
            <label for="rating">Rating</label>
            <input type="number" id="rating" value="rating">
            <label for="salary">Salary</label>
            <input type="number" id="salary" value="salary">
            <input type="button" id="submit" value="Submit">
        </form>
    </div>